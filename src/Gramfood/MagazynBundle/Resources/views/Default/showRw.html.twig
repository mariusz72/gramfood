         <tbody>       
         {% set sum = 0 %}     
                 {% for entity in entities %}    
             <tr>
             <td border="0">{{ loop.index }}</td>
                 <td width="200px" >{{ entity.id }}</td>           
                 <td width="20px">{{ entity.typ }}</td>
                 <td width="30px">{{ entity.kod }}</td>
                 <td >{{ entity.nazw }}</td>           
                 <td width="20px">{{ entity.il }}</td>
                 <td width="20px">{{ entity.jm }}</td>   
                 <td> <button type="button" class="btn btn-default btn-sm" onClick="wyswietl_nrpartii('{{ entity.id }}','{{ entity.nazw }}','{{ idkpl }}')">NR PARTII</button></td>   
             </tr> 
             {% set sum = sum + entity.il  %} 
            {% endfor %}
             <tr>
                 <td colspan="5"></td>
                 <td style="background-color: beige;">{{ sum }}</td>
                 <td style="background-color: beige;">{{ entities[0].jm }}</td>   
                 <td ></td>   
             </tr>            
         </tbody>
     </table>
     
   
     

           
 


   


