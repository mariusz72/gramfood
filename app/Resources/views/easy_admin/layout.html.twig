<!DOCTYPE html>
<html lang="en">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <!-- Meta, title, CSS, favicons, etc. -->
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <title>WMS App</title>


    <!-- Bootstrap -->
    <link href="{{ asset('bundles/gentelella-master/vendors/bootstrap/dist/css/bootstrap.min.css') }}" rel="stylesheet">
    <!-- Font Awesome -->
    <link href="{{ asset('bundles/gentelella-master/vendors/font-awesome/css/font-awesome.min.css') }}" rel="stylesheet">
{% block stylesheets %}	
{% endblock stylesheets %}

    <!-- Custom Theme Style -->
    <link href="{{ asset('bundles/gentelella-master/build/css/custom.min.css') }}" rel="stylesheet">
<style>

.img-logo 
{
    width: 70%;
    margin-left: 5%;
    z-index: 1000;
    position: inherit;
    margin-top: 10px;
    padding: 4px ;
	border-radius: 0%;
}
	
</style>
	
	
</head>

<body class="nav-md">
<div class="container body">
    <div class="main_container">


        {% include 'gramfood/sidebar.html.twig' %}
        {% include 'gramfood/topnav.html.twig' %}


        <!-- page content -->
        <div class="right_col" role="main">
            <div class="">
                {% block top_content_header %}
                <div class="page-title">
                    <div class="title_left">
                   <h3>{% block page_title %}{% endblock page_title %}</h3>
                    </div>


                </div>
                {% endblock top_content_header %}

                <div class="clearfix"></div>


                {% block page_content %}
                
 <!-- /page content -->



        <div class="wrapper">
        {% block wrapper %}


            <div class="content-wrapper">
            {% block content %}
                {% block flash_messages %}
                    {{ include(_entity_config is defined ? _entity_config.templates.flash_messages : '@EasyAdmin/default/flash_messages.html.twig') }}
                {% endblock flash_messages %}

                <section class="content-header">
                {% block content_header %}
                    <h1 class="title">{% block content_title %}{% endblock %}</h1>
                {% endblock content_header %}
                {% block content_help %}
                    {% if _entity_config is defined and _entity_config[app.request.query.get('action')]['help']|default(false) %}
                        <div class="box box-widget help-entity">
                            <div class="box-body">
                                {{ _entity_config[app.request.query.get('action')]['help']|trans|raw }}
                            </div>
                        </div>
                    {% endif %}
                {% endblock content_help %}
                </section>

                <section id="main" class="content">
                    {% block main %}{% endblock %}
                </section>
            {% endblock content %}
            </div>
        {% endblock wrapper %}
        </div>

        {% block body_javascript %}{% endblock body_javascript %}

        {% for js_asset in easyadmin_config('design.assets.js') %}
            <script src="{{ asset(js_asset) }}"></script>
        {% endfor %}




<!-- /page content -->               
                
                {% endblock %}


            </div>
        </div>

        {% include 'gramfood/footer.html.twig' %}


    </div>
</div>

{% block javascripts %}
    <!-- jQuery -->
    <script src="{{ asset('bundles/gentelella-master/vendors/jquery/dist/jquery.min.js') }}"></script>
    <!-- Bootstrap -->
    <script src="{{ asset('bundles/gentelella-master/vendors/bootstrap/dist/js/bootstrap.min.js') }}"></script>
    <!-- FastClick -->
    <script src="{{ asset('bundles/gentelella-master/vendors/fastclick/lib/fastclick.js') }}"></script>
    <!-- NProgress -->
    <script src="{{ asset('bundles/gentelella-master/vendors/nprogress/nprogress.js') }}"></script>
    <!-- Custom Theme Scripts -->
    <script src="{{ asset('bundles/gentelella-master/build/js/custom.min.js') }}"></script>
	
{% endblock javascripts %}

{% block modals %}{% endblock modals %}


</body>
</html>





